##
## /opt/bro/share/bro/intel/__load__.bro
##
## Generated by Chef for <%= node[:fqdn] %>
##

@load frameworks/intel/seen
@load frameworks/intel/do_notice
@load frameworks/files/hash-all-files

redef Intel::read_files += {
	<% if node[:seconion][:sensor][:bro_scripts][:intel] %>
		<% node[:seconion][:sensor][:bro_scripts][:intel].each do |intel_filename| %>
	"/opt/bro/share/bro/intel/<%= intel_filename %>"
		<% end %>
	<% else %>
	"/opt/bro/share/bro/intel/intel.dat"
	<% end %>
<% end %>
};
