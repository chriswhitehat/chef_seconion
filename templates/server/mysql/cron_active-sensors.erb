#
# /etc/cron.d/active-sensors
#
# Generated by Chef for <%= node[:fqdn] %>
#
#

SHELL=/bin/sh
PATH=/usr/local/sbin:/usr/local/bin:/sbin:/bin:/usr/sbin:/usr/bin

*/5 * * * * root /usr/bin/mysql -u root -A  -D <%= node[:seconion][:server][:sguil_server_name] %>_db -e 'SELECT net_name FROM sensor WHERE active="Y";' | egrep -v net_name | sort | uniq > /etc/nsm/active_sensors
